<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- 获取焦点时候按钮的背景状态 -->
    <item android:drawable="@drawable/fillet_shape_action_button_background_with_alph_pressed" android:state_enabled="true" android:state_focused="true"/>
    <!-- 被按下时候按钮的背景状态 -->
    <item android:drawable="@drawable/fillet_shape_action_button_background_with_alph_pressed" android:state_enabled="true" android:state_pressed="true"/>
    <!-- 正常状态下按钮的状态 -->
    <item android:drawable="@drawable/fillet_shape_action_button_background_with_alph_normal"/>

</selector>